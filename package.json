        "exclude": [
            "node_modules",
            ".babelrc",
            ".eslintrc",
            "test",
            ".vscode",
            ".next",
            "package",
            "platform_www",
            "platforms",
            "plugins",
            "dist"
        ],
        "prefix": "> 通过npm run cpfs 命令创建，请不要手动维护此文件。\r\n\r\n\r\n",
        "suffix": "",
        "outFileName": "文件结构.md"
